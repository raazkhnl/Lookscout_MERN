{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    " redirects": [
      { "source": "/(.*)", "destination": "https://lookscout-mern.vercel.app/$1", "permanent": true }
    ]
  }
  